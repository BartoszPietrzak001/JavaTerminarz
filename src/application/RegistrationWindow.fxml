<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RegistrationController">
   <children>
      <AnchorPane fx:id="root" layoutX="2.0" layoutY="-1.0" prefHeight="448.0" prefWidth="406.0">
         <children>
            <Label fx:id="communicateLabel" alignment="CENTER" layoutX="46.0" layoutY="65.0" prefHeight="21.0" prefWidth="312.0" text="Enter user info" />
            <Button fx:id="backToLoginButton" layoutX="301.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToLoginWindow" prefHeight="31.0" prefWidth="90.0" text="Back" />
            <TextField fx:id="userNameLabel" layoutX="108.0" layoutY="126.0" promptText="UserName" />
            <TextField fx:id="emailLabel" layoutX="111.0" layoutY="236.0" promptText="E-Mail" />
            <Button fx:id="submitButton" layoutX="153.0" layoutY="314.0" mnemonicParsing="false" onAction="#submit" prefHeight="31.0" prefWidth="90.0" text="Submit" />
            <PasswordField fx:id="passwordLabel" layoutX="110.0" layoutY="180.0" promptText="Password" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
